      $$ = new std::vector<AudioCodec>();
      $$->push_back(*$1);
      DELETE_TOKEN($1);
    }
  | wfd_audio_codec_list ',' WFD_SP wfd_audio_codec {
      UNUSED_TOKEN($$);
      $1->push_back(*$4);
      DELETE_TOKEN($4);
    }
  ;

wfd_audio_codec:
