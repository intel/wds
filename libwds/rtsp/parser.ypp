  | wfd_uibc_setting
  | wfd_standby_resume_capability
  | wfd_connector_type
  | WFD_STANDBY_IN_RESPONSE {
      $$ = new Standby();
    }
  | WFD_IDR_REQUEST {
      $$ = new IDRRequest();
    }
  | WFD_GENERIC_PROPERTY WFD_STRING {
      $$ = new GenericProperty(*$1, *$2);
      DELETE_TOKEN($1);
      DELETE_TOKEN($2);
    }
  ;

wfd_property_audio_codecs:
