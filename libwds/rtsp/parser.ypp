  | WFD_IDR_REQUEST_ERROR ':' WFD_SP wfd_error_list {
      $$ = new PropertyErrors(WFD_IDR_REQUEST, *$4);
      DELETE_TOKEN($4);
    }
  | WFD_GENERIC_PROPERTY_ERROR ':' WFD_SP wfd_error_list {
      $$ = new PropertyErrors(*$1, *$4);
      DELETE_TOKEN($1);
      DELETE_TOKEN($4);
    }
